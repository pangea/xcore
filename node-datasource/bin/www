#!/usr/bin/env node
var debug = require('debug')('node-datasource');
var app   = require('../app');
var https = require('https');
var fs    = require('fs');
var server = https.createServer({
      key: fs.readFileSync('./lib/private/key.pem'),
      cert: fs.readFileSync('./lib/private/server.crt')
    }, app.router);

app.socket.attach(server);

server.listen(8443);
